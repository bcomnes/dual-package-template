{
  "name": "{{name}}",
  "description": "WIP - nothing to see here",
  "version": "0.0.0",
  "type": "module",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io)",
  "bugs": {
    "url": "https://github.com/bcomnes/{{name}}/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "ascjs": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "dependency-check": "^4.1.0",
    "standard": "^14.3.3",
    "auto-changelog": "^2.0.0",
    "tap": "^14.10.8",
    "gh-release": "^4.0.0"
  },
  "homepage": "https://github.com/bcomnes/{{name}}",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcomnes/{{name}}.git"
  },
  "scripts": {
    "build": "rimraf cjs/* && ascjs esm cjs",
    "prepare": "npm run build",
    "prepublishOnly": "git push --follow-tags && gh-release",
    "test": "run-s build test:*",
    "test:deps": "dependency-check . --no-dev --no-peer",
    "test:standard": "standard",
    "test:tap": "tap --no-esm --jobs=1",
    "version": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:' && git add CHANGELOG.md cjs"
  },
  "semistandard": {
    "ignore": [
      "dist"
    ]
  },
