{
  "name": "{{name}}",
  "description": "WIP - nothing to see here",
  "version": "0.0.0",
  "type": "module",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io)",
  "bugs": {
    "url": "https://github.com/bcomnes/{{name}}/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "ascjs": "^6.0.2",
    "npm-run-all2": "^6.0.0",
    "dependency-check": "^4.1.0",
    "standard": "^17.0.0",
    "auto-changelog": "^2.0.0",
    "tap": "^16.0.0",
    "gh-release": "^7.0.0",
    "c8": "^8.0.0"
  },
  "homepage": "https://github.com/bcomnes/{{name}}",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcomnes/{{name}}.git"
  },
  "scripts": {
    "build": "rm -rf cjs && mkdir -p cjs && ascjs esm cjs",
    "prepare": "npm run build",
    "prepublishOnly": "git push --follow-tags && gh-release -y",
    "test": "run-s build test:*",
    "test:deps": "dependency-check package.json --no-dev --no-peer",
    "test:standard": "standard",
    "test:tap": "METRICS=0 c8 tap --no-coverage --color",
    "version": "run-s prepare version:*",
    "version:changelog": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:'",
    "version:git": "git add CHANGELOG.md cjs"
  },
  "standard": {
    "ignore": [
      "dist",
      "cjs"
    ]
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/bcomnes"
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
